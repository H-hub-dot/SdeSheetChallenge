/*********************************************************************Unique Paths***************************************************************************************/


int uniquePaths(int m, int n)
    {
        int N = n+m-2; 
        int r = min(n,m) - 1; 
        int res = 1; 
        for(int i=1; i<=r; ++i, N--){
            res = res * (N) / i;
        }return res;
    }
